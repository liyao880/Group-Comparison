getK=function(win_player,lose_player){
win=as.vector(as.matrix(win_player))
lose=as.vector(as.matrix(lose_player))
total=c(win,lose)
tb=table(total)
#K=as.vector(tb)
K=as.data.frame(tb)
return(K)
}
